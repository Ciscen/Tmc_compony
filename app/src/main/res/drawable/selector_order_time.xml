<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <bitmap android:gravity="top" android:src="@mipmap/order_time_checked" android:tileMode="disabled" />
    </item>
    <item android:state_checked="false">
        <bitmap android:gravity="top" android:src="@mipmap/order_time_unchecked" android:tileMode="disabled" />
    </item>
</selector>