<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleView">
        <attr name="title" format="string" />
        <attr name="subTitle" format="string" />
        <attr name="moreimage" format="reference" />
        <attr name="bgColor" format="reference" />
    </declare-styleable>
    <declare-styleable name="MyCheckbox">
        <attr name="text" format="string" />
    </declare-styleable>
    <declare-styleable name="ItemView">
        <attr name="itemTitle" format="string" />
        <attr name="content" format="string" />
        <attr name="hint" format="string" />
        <attr name="inputable" format="boolean" />
        <attr name="itemTitleColor" format="string" />
        <attr name="isPassword" format="boolean" />
        <attr name="leftpadding" format="dimension" />
        <attr name="hasDivider" format="boolean" />
    </declare-styleable>
    <declare-styleable name="TrainDetailCardView">
        <attr name="timeOrSeattype" format="integer" />
    </declare-styleable>
    <declare-styleable name="PlaneDetailCardView">
        <attr name="type" format="integer" />
    </declare-styleable>
    <declare-styleable name="RangeSeekBar">
        <attr name="orientation" format="string" />
        <attr name="limitThumbRange" format="boolean" />
        <attr name="scaleMin" format="float" />
        <attr name="scaleMax" format="float" />
        <attr name="scaleStep" format="float" />
        <attr name="thumb" format="reference" />
        <attr name="thumbs" format="integer" />
        <attr name="thumbWidth" format="dimension" />
        <attr name="thumbHeight" format="dimension" />
        <attr name="track_rsb" format="reference" />
        <attr name="range" format="reference" />
        <attr name="textSize" format="dimension" />
        <attr name="trackHeihgt" format="dimension" />
    </declare-styleable>
    <declare-styleable name="StateView">
        <attr name="imgOn" format="reference" />
        <attr name="imgOff" format="reference" />
        <attr name="viewName" format="string" />
        <attr name="isOn" format="boolean" />

    </declare-styleable>

</resources>